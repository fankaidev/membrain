    "@google/generative-ai": "^0.5.0",
    "@mozilla/readability": "^0.5.0",
    "@types/lodash": "^4.17.0",
    "@types/markdown-it": "^14.0.0",
    "@types/uuid": "^9.0.8",
    "antd": "^5.16.1",
    "husky": "^9.0.11",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
